  "version": "0.2.0",
  "configurations": [

    {
      "name": "Attach to Chrome",
      "port": 5500,
      "request": "attach",
      "type": "chrome",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "Launch Chrome",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:5500/pages/profile.html",
      "webRoot": "${workspaceFolder}"
    }

  ]
}
